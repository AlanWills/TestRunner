<Application x:Class="TestWPFApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TestRunner.UserControls"
             StartupUri="Views/MainWindow.xaml">

    <Application.Resources>
        <local:NavBar x:Key="NavBar"/>
        <local:FileOpen x:Key="FileOpen"/>
        <local:FileSave x:Key="FileSave"/>
        <ResourceDictionary x:Key="Styles" Source="ResourceDictionaries/TestRunnerStyles.xaml"/>
    </Application.Resources>

</Application>
